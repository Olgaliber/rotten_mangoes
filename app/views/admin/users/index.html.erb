<h1> ADMIN PAGE </h1>

 <div class="container">
     <div class="users row">
     		<table>
         <%= paginate(@users) %>
         <% @users.each do |user| %>
         <div class="panel panel-default">
     				<tr>
             <td><%= user.full_name %></td>  
             <td><%= user.email %></td>
             <td><%= user.updated_at %></td>
             <td> <%= link_to "Delete user", admin_user_path(user), method: :delete, data: {confirm: "You sure?"} %> </td>
            </tr>
         </div>
         <% end %>
       	</table>
     </div>
 </div>
